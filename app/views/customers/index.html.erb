<h2>会員一覧</h2>
 <table class="table table-hover table-inverse">
  <thead>
   <tr>
    <th >会員ID</th>
    <th >氏名</th>
    <th>メールアドレス</th>
　　<th>ステータス</th>
　　</thead>
　 <tbody>
  <% @customer.each do |customer| %>
  <tr>
  <td><%= customer.id %></td>
  <td>
  <%= customer.name %> 
  </td>
  <td>
  <%= customer.email %>
  </td>
  <td>
  <%= customer.is_active %>
  </td>
  
  
  <% end %>